{
    "name": "limpalair/stripe",
    "description": "Limpalair Stripe provides an interface to Stripe's payment system in Laravel.",
    "keywords": ["laravel", "stripe", "billing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Limpalair",
            "email": "christf24@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/filesystem": "~5.0",
        "illuminate/support": "~5.0",
        "stripe/stripe-php": "~3.0",
        "symfony/http-kernel": "~2.3"
    },
    "require-dev": {
        "illuminate/routing": "~5.0",
        "illuminate/view": "~5.0",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "Limpalair\\Stripe\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}